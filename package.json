{
  "name": "fcc-life",
  "version": "1.0.0",
  "description": "Comway's Game of Life implementation for FreeCodeCamp's Certification",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && webpack -p",
    "clean": "rimraf build",
    "serve": "webpack-dev-server --config webpack.config.dev.js",
    "deploy:gh-pages": "npm run build && ren build buildx && git checkout gh-pages && rimraf build && ren buildx build && git add . && git commit --message=\"Commit to gh-pages branch\" && git subtree push --prefix build github gh-pages && git checkout master",
    "deploy:surge": "surge -p build"
  },
  "author": "Walter",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "image-webpack-loader": "^3.3.1",
    "node-sass": "^4.5.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.5.9",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "foundation-sites": "6.4.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  }
}
